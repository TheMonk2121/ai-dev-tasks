# Repo-Gold Evaluation Configuration
# Enables strict oracle gates for accuracy measurement

# Dataset Configuration
DATASET_HAS_GOLD=1
EVAL_DATASET=datasets/dev_gold.jsonl
FEW_SHOT_POOL=datasets/few_shot_pool.jsonl

# Oracle Thresholds (strict gates)
ORACLE_CTX_J_MIN=0.28
ORACLE_SENT_J_MIN=0.32
ORACLE_PREFILTER_TARGET=0.85
READER_USED_GOLD_TARGET=0.70

# File-Oracle (primary for repo-gold)
FILE_ORACLE_PREFILTER_MIN=0.85
FILE_ORACLE_READER_MIN=0.70

# Evaluation Thresholds
F1_SCORE_MIN=0.22
PRECISION_DRIFT_MAX=0.02
LATENCY_INCREASE_MAX=0.15
TOOL_SCHEMA_CONFORMANCE_MIN=0.95

# RAG Configuration
RERANK_ENABLE=1
RETR_TOPK_VEC=140
RETR_TOPK_BM25=140
RERANK_POOL=60
RERANK_TOPN=18
CONTEXT_DOCS_MAX=12

# Evaluation Settings
EVAL_DISABLE_CACHE=1
TEMPERATURE=0
FEW_SHOT_K=0
COT_ENABLED=false
EVAL_DRIVER=dspy_rag
RAGCHECKER_USE_REAL_RAG=1

# Leakage Guard
ENABLE_LEAKAGE_GUARD=1
LEAKAGE_ASSERT_CLEAN=1

# Logging
RAGCHECKER_PROGRESS_LOG=logs/repo_gold_evaluation.jsonl
DEBUG_FUSION=0

# Model Configuration
RERANK_MODEL=BAAI/bge-reranker-base
EMBEDDER_MODEL=intfloat/e5-large-v2

# Database Configuration
POSTGRES_DSN=postgresql://danieljacobs@localhost:5432/ai_agency?sslmode=disable
